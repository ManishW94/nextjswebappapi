generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  // id       String @id @default(uuid()) @db.Uuid
  id       String @id
  name     String
  role     Role   @default(USER)
  avatar   String
  username String @unique
  password String
}

enum Role {
  USER
  ADMIN
}
